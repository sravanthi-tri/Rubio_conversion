      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="step 08 Add Caption"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:PropertyExpression
        DTS:Name="SqlStatementSource">"UPDATE STY  
SET STY.Plaintiff = C_CP.[plaintiff_caption], STY.defendant = C_CP.[defendant_caption], 
STY.caseno = C_CP.[case_no_caption], 
STY.[Juridictional Title] = C_CP.[judicial_caption], 
STY.Re = C_CP.[certificate_caption], 
STY.ReNumberTemp = C_CP.[case_parties_sk]
FROM dbo.Style STY INNER JOIN dbo.CaseTable CT ON STY.CaseId = CT.CaseId  
INNER JOIN "+@[$Project::Source_DB] +".dbo.case_parties CP ON CT.JurisMatSysNum = CP.case_sk
INNER JOIN "+@[$Project::Source_DB] +".dbo.case_caption C_CP ON CP.case_parties_sk = C_CP.case_parties_sk WHERE (LEN(LTRIM(RTRIM(CAST(judicial_caption AS varchar(max)))))) &lt; 255"</DTS:PropertyExpression>
