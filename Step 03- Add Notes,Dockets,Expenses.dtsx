    <DTS:Executable
      DTS:refId="Package\step 06b1 add Docketassignedto"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{9da7edca-06b7-481a-a677-c4cff41227bb}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="step 06b1 add Docketassignedto"
      DTS:ThreadHint="1">
      <DTS:Variables />
      <DTS:PropertyExpression
        DTS:Name="SqlStatementSource">"INSERT INTO dbo.DocketAssignedTo ( docketid, AssignedTo, ts )
SELECT DISTINCT DK.docketid, EMP.initials AS AssignedTo, getdate() AS ts
FROM "+@[$Project::Source_DB] +".dbo.employee EMP 
INNER JOIN "+@[$Project::Source_DB] +".dbo.appt_exch AE ON EMP.employee_sk = AE.appoint_user_sk
INNER JOIN dbo.Docket DK  ON AE.Appt_Assoc_Sk = DK.idfrom 
WHERE EMP.initials Is Not Null
AND AE.appoint_user_sk Is Not Null 
AND DK.filetab ='Scheduled Event' 
AND DK.DocketId not in (Select DocketId from  dbo.DocketAssignedTo  ) "</DTS:PropertyExpression>
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{47CCE84F-7FE6-4105-870B-4F4543F4FADF}"
          SQLTask:SqlStatementSource="INSERT INTO dbo.DocketAssignedTo ( docketid, AssignedTo, ts )&#xA;SELECT DISTINCT DK.docketid, EMP.initials AS AssignedTo, getdate() AS ts&#xA;FROM [DATACONVERSION\SQLEXPRESS2014].[MBBCPSQL].dbo.employee EMP &#xA;INNER JOIN [DATACONVERSION\SQLEXPRESS2014].[MBBCPSQL].dbo.appt_exch AE ON EMP.employee_sk = AE.appoint_user_sk&#xA;INNER JOIN dbo.Docket DK  ON AE.Appt_Assoc_Sk = DK.idfrom &#xA;WHERE EMP.initials Is Not Null&#xA;AND AE.appoint_user_sk Is Not Null &#xA;AND DK.filetab ='Scheduled Event' &#xA;AND DK.DocketId not in (Select DocketId from  dbo.DocketAssignedTo  ) " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
