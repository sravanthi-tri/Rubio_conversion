    <DTS:Executable
      DTS:refId="Package\step 06a Add DocketAssignedto- to dos"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{f1073fef-b08f-4c5c-976f-2c98047976da}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="step 06a Add DocketAssignedto- to dos"
      DTS:ThreadHint="1">
      <DTS:Variables />
      <DTS:PropertyExpression
        DTS:Name="SqlStatementSource">" INSERT INTO dbo.DocketAssignedTo ( DocketId, AssignedTo )
SELECT DK.DocketId, EMP.initials
FROM dbo.Docket DK  INNER JOIN "+@[$Project::Source_DB] +".dbo.assoc_todo_emps ATE  ON DK.idfrom = ATE.to_do_sk
INNER JOIN "+@[$Project::Source_DB] +".dbo.employee EMP ON ATE.employee_sk = EMP.employee_sk 
WHERE EMP.initials &gt;'0'  AND DK.FileTab ='ToDO' 
AND DK.DocketId not in (Select DocketId from  dbo.DocketAssignedTo  )"</DTS:PropertyExpression>
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{47CCE84F-7FE6-4105-870B-4F4543F4FADF}"
          SQLTask:SqlStatementSource=" INSERT INTO dbo.DocketAssignedTo ( DocketId, AssignedTo )&#xA;SELECT DK.DocketId, EMP.initials&#xA;FROM dbo.Docket DK  INNER JOIN [DATACONVERSION\SQLEXPRESS2014].[MBBCPSQL].dbo.assoc_todo_emps ATE  ON DK.idfrom = ATE.to_do_sk&#xA;INNER JOIN [DATACONVERSION\SQLEXPRESS2014].[MBBCPSQL].dbo.employee EMP ON ATE.employee_sk = EMP.employee_sk &#xA;WHERE EMP.initials &gt;'0'  AND DK.FileTab ='ToDO' &#xA;AND DK.DocketId not in (Select DocketId from  dbo.DocketAssignedTo  )" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
